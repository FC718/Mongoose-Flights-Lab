<%- include('../partials/header') %>

    <h2>FLIGHTS</h2>
    <table>
        <thead>
            <tr>
                <th>Airline</th>
                <th>Airport</th>
                <th>FlightNo</th>
        <%- include('../partials/header') %>
          <th>Departs</th>
            </tr>
        </thead>
    <tbody>
        <% flights.forEach(function(f) { %>
            <tr> 
                <td><%= f.airline%></td>
                <td><%= f.airport%></td>
                <td><%= f.flightNo%></td>
                <td><%= f.departs%></td>
                <td><a href="/flights/<%=f._id%>">Details</a></td>
            </tr>

       <% }); %>
    </tbody>
</table>

    <%- include('../partials/footer') %>